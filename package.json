      "name": "Tamás László"
    }
  ],
  "homepage": "https://tolads.github.io/my-movie-lists",
  "dependencies": {
    "@material-ui/core": "4.9.1",
    "@material-ui/icons": "4.9.1",
    "mobx": "5.15.4",
    "mobx-react-lite": "1.5.2",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-select": "3.0.8"
  },
  "devDependencies": {
    "@cypress/code-coverage": "1.11.0",
    "@cypress/instrument-cra": "1.0.0",
    "customize-cra": "0.9.1",
    "cypress": "3.8.3",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.1",
    "istanbul-lib-coverage": "3.0.0",
    "lint-staged": "10.0.7",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "react-app-rewired": "2.1.5",
    "react-scripts": "3.3.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "start-for-cypress-coverage": "react-app-rewired -r @cypress/instrument-cra start",
    "build": "react-app-rewired build",
    "test": "cypress run",
    "cypress:open": "cypress open",
    "lint-js": "node node_modules/eslint/bin/eslint.js src/ --config .eslintrc.js",
    "eject": "react-scripts eject"
  },
  "lint-staged": {
    "{src,cypress}/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions",
      "> 1%"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  }
}
